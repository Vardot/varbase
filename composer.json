    "drupal/features": "3.5",
    "drupal/views_infinite_scroll": "1.3",
    "drupal/embed": "1.0",
    "drupal/entity": "1.0-alpha4",
    "drupal/entity_browser": "1.0-rc2",
    "drupal/dropzonejs": "1.0-alpha6",
